{
  "manifest_version": 3,
  "name": "Hci url redirect",
  "version": "1.0",
  "permissions": ["storage", "tabs", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "options.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["getTab.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
